package kr.inhatc.spring.user.entity;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;
import lombok.NoArgsConstructor;

@Entity
@Table(name = "user")
@NoArgsConstructor
@Data
public class User {
	
	
	@Id
	private String id;
	private String pw;
	private String name;
	private String email;
	

}
